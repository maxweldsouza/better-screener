{
  "manifest_version": 3,
  "name": "Better Screener",
  "version": "1.0",
  "description": "Screener.in enhancements.",
  "permissions": [],
  "host_permissions": [
    "https://www.screener.in/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.screener.in/*"],
      "js": ["js/chart.umd.js", "content.js"],
      "run_at": "document_idle"
    }
  ]
}